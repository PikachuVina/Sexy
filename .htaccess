RewriteEngine On
ErrorDocument 400 /_cnF/_star_loi.php
ErrorDocument 401 /_cnF/_star_loi.php
ErrorDocument 402 /_cnF/_star_loi.php
ErrorDocument 403 /_cnF/_star_loi.php
ErrorDocument 404 /_cnF/_star_loi.php
ErrorDocument 500 /_cnF/_star_loi.php
ErrorDocument 502 /_cnF/_star_loi.php
ErrorDocument 503 /_cnF/_star_loi.php
ErrorDocument 504 /_cnF/_star_loi.php
RewriteRule ^@FILEHETHONG@$ @FILEHETHONG@.php [L]
RewriteRule ^@TIMKIEM@$ @TIMKIEM@.php [L]
RewriteRule ^@TIENTRINH@$ @TIENTRINH@.php [L]
RewriteRule ^error$ _cnF/loi.php [L]
RewriteRule ^index.html$ index.php [L]
RewriteRule ^trangchu.html$ home.php [L]
RewriteRule ^get-token.html$ login/gettoken.php [L]
RewriteRule ^login-captcha.html$ login/captcha-login.php [L]
RewriteRule ^check-token.html$ tools/Vip/check.php [L]
RewriteRule ^([a-zA-Z_-]+)-([a-zA-Z_-]+).html$ home.php?act=$1&id=$2

<Files ~ "^.*\.(php|cgi|pl|php3|php4|php5|php6|phtml|shtml)">
Order allow,deny
Deny from all
</Files>

<Files ^(*.jpeg|*.jpg|*.png|*.gif)>
order deny,allow
deny from all
</Files>

Options -Indexes
Options -ExecCGI 
AddHandler cgi-script .php .php3 .php4 .phtml .pl .py .jsp .asp .htm .shtml .sh .cgi

<Files "config.php">
Order Deny,Allow
Deny from All
</Files>